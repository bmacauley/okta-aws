{
  "rules": {
    "indent": [ 2, 2, { "SwitchCase": 1 } ],
    "linebreak-style": [ 2, "unix" ],
    "no-var": 2,
    "object-curly-spacing": [2, "always"],
    "quotes": [ 2, "single" ],
    "semi": [ 2, "always" ],
    "no-console": 1
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "describe": false,
    "it": false
  }
}
